{
  "name": "raicem/laravel-defer-jobs",
  "description": "Defer your jobs to be executed after the response is sent. Without a queue system.",
  "keywords": [
    "laravel",
    "queue",
    "deferring"
  ],
  "homepage": "https://github.com/raicem/laravel-defer-jobs",
  "license": "MIT",
  "authors": [
    {
      "name": "Cem Ãœnalan",
      "email": "unalancem@gmail.com",
      "homepage": "https://github.com/raicem/laravel-defer-jobs",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "autoload": {
    "psr-4": {
      "Raicem\\Defer\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Raicem\\Defer\\DeferServiceProvider"
      ]
    }
  },
  "scripts": {
    "format": "./vendor/bin/php-cs-fixer fix src"
  }
}
